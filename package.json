  "scripts": {
    "add-geckodriver-to-path": "export PATH=$PATH:$PWD/node_modules/geckodriver/bin",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "make lint",
    "init-server": "./test/functional/start_http_server & echo $! > pid",
